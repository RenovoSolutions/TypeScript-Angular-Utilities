{
  "name": "typescript-angular-utilities",
  "version": "3.1.5",
  "description": "Typescript utility classes published as angular services",
  "author": "Renovo Development Team",
  "main": "source/main.js",
  "typings": "source/main.d.ts",
  "keywords": [
    "typescript",
    "utilities"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/RenovoSolutions/TypeScript-Angular-Utilities"
  },
  "scripts": {
    "prepublish": "npm run build",
    "bundle-deps": "gulp bundle-tests-vendor && gulp bundle-tests-renovo",
    "update": "npm install && typings install && npm run bundle-deps",
    "version": "gulp version -v ",
    "clean": "gulp clean",
    "build": "gulp clean && tsc && echo Build complete",
    "build.watch": "tsc -w",
    "bundle-tests": "gulp bundle-tests",
    "test": "npm run bundle-tests && karma start",
    "test.debug": "concurrently --kill-others \"gulp bundle-tests.watch\" \"karma start karma.debug.conf.js\"",
    "test.tc": "npm run bundle-tests && karma start karma.tc.conf.js",
    "test.full": "npm run bundle-tests && karma start karma.full.conf.js",
    "install-build": "npm install && npm run build",
    "install-build-test": "npm install && npm run build && npm run test",
    "build-test": "npm run build && npm run test",
    "build-test.watch": "concurrently --kill-others \"npm run build.watch\" \"npm run test.debug\"",
    "full-build": "npm install && npm run build",
    "all": "npm run update && npm run build && npm run test.full"
  },
  "devDependencies": {
    "@renovolive/gulp-utilities": "~2.1.10",
    "chai": "~3.5.0",
    "concurrently": "~2.0.0",
    "del": "~2.2.0",
    "gulp": "~3.9.1",
    "karma": "~0.13.21",
    "karma-chai": "~0.1.0",
    "karma-chrome-launcher": "~0.2.2",
    "karma-firefox-launcher": "~0.1.7",
    "karma-ie-launcher": "~0.2.0",
    "karma-mocha": "~0.2.2",
    "karma-sinon": "~1.0.4",
    "karma-teamcity-reporter": "~0.2.1",
    "mocha": "~2.4.5",
    "run-sequence": "^1.1.5",
    "sinon": "~1.17.3",
    "systemjs-builder": "^0.15.16",
    "typescript": "~1.8.2",
    "typings": "^1.0.4"
  },
  "dependencies": {
    "@angular/compiler": "^2.0.0-rc.2",
    "@angular/core": "^2.0.0-rc.2",
    "@angular/http": "^2.0.0-rc.2",
    "@angular/upgrade": "^2.0.0-rc.2",
    "@angular/platform-browser": "^2.0.0-rc.2",
    "@angular/common": "^2.0.0-rc.2",
    "angular2-uuid": "^1.0.7",
    "angular": "^1.5.3",
    "angular-mocks": "~1.5.0",
    "es6-shim": "^0.35.0",
    "json-loader": "~0.5.4",
    "lodash": "~4.5.1",
    "moment": "~2.11.2",
    "moment-timezone": "~0.5.3",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.6",
    "zone.js": "^0.6.12"
  },
  "license": "MIT"
}
